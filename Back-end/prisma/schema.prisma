generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model product {
  id            Int     @unique @default(autoincrement())
  nome          String? @db.VarChar(45)
  preco_venda   Int
  qtd           Int
  preco_compra  Int
  porcent_lucro Int
  categoria     String  @db.VarChar(45)
}

model categorias {
  id   Int     @id @default(autoincrement())
  nome String? @db.VarChar(45)
}
